{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "sentiment-analysis-platform",
  "compatibility_date": "2025-09-25",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "sentiment-db",
      "database_id": "your-database-id-here"
    }
  ],
  "ai": {
    "binding": "AI"
  }
}